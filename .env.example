# EVE Developer Application Client ID
# Get this from https://developers.eveonline.com/
EVE_CLIENT_ID=your_client_id_here

# OAuth callback URL (default is fine for local development)
EVE_CALLBACK_URL=http://localhost:8080/callback

# How often to refresh calendar data from ESI (in minutes)
CALENDAR_REFRESH_MINUTES=15

# Optional: Provide your own encryption key for token storage
# If not set, one will be auto-generated and stored in data/encryption.key
# TOKEN_ENCRYPTION_KEY=your_base64_encoded_32_byte_key
